# general shortcuts

alias serve='python -m SimpleHTTPServer 4321'
alias killswap='rm /var/tmp/*.sw* | rm ./*.sw*'
alias daynotes="vim ~/notes/$( date '+%d-%m_' )notes.md"
alias c='clear'
alias sand='cd ~/sandbox'

# git shortcuts!

alias gl='git log --graph --pretty=oneline'

# meta:

alias aliases='less ~/.aliases'
