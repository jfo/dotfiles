[user]
	name = Jeff Fowler
	email = jeffowler@protonmail.com

[core]
    excludesfile = ~/.gitignore

[includeIf "gitdir:~/development/*"]
    path = ~/.gitconfig-work

[alias]
    co = checkout
    l = log --oneline
    rpull = pull --rebase
    touched = "!f() { git diff --name-only $(git merge-base HEAD master); }; f"
    diffm = "!f() { git diff $(git merge-base HEAD master); }; f"
    cp = cherry-pick
[hub]
    protocol = https
